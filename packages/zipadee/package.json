{
  "name": "zipadee",
  "description": "Zipadee is a web server framework for Node.js",
  "version": "0.0.21",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "keywords": [],
  "author": "",
  "homepage": "https://github.com/justinfagnani/zipadee#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinfagnani/zipadee.git",
    "directory": "packages/zipadee"
  },
  "license": "MIT",
  "files": [
    "/index.{d.ts.map,d.ts,js.map,js}"
  ],
  "wireit": {
    "build": {
      "command": "tsc",
      "dependencies": [
        "../core:build",
        "../cors:build",
        "../router:build",
        "../static:build"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "index.{js,js.map,d.ts,d.ts.map}",
        ".tsbuildinfo",
        "lib",
        "test"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "node --enable-source-maps --test --test-reporter=spec \"test/**/*_test.js\"",
      "dependencies": [
        "build"
      ],
      "files": []
    }
  },
  "dependencies": {
    "@zipadee/core": "^0.0.21",
    "@zipadee/cors": "^0.0.21",
    "@zipadee/router": "^0.0.21",
    "@zipadee/static": "^0.0.21"
  }
}
