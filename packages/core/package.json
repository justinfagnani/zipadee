{
  "name": "@zipadee/core",
  "version": "0.0.21",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "keywords": [],
  "author": "",
  "homepage": "https://github.com/justinfagnani/zipadee#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinfagnani/zipadee.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "description": "The core components of Zipadee, a web server framework for Node.js",
  "files": [
    "/index.{d.ts.map,d.ts,js.map,js}",
    "/lib/"
  ],
  "devDependencies": {
    "@types/accepts": "^1.3.7",
    "@types/cookies": "^0.9.0",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0"
  },
  "wireit": {
    "build": {
      "command": "tsc",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "index.{js,js.map,d.ts,d.ts.map}",
        ".tsbuildinfo",
        "lib",
        "test"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "node --enable-source-maps --test --test-reporter=spec \"test/**/*_test.js\"",
      "dependencies": [
        "build"
      ],
      "files": []
    }
  },
  "dependencies": {
    "accepts": "^1.3.8",
    "cache-content-type": "^2.0.0",
    "cookies": "^0.9.1"
  }
}
