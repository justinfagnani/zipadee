{
  "name": "@zipadee/trpc",
  "version": "0.0.1",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@types/accepts": "^1.3.7",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0"
  },
  "wireit": {
    "build": {
      "command": "tsc",
      "dependencies": [
        "../core:build"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "index.{js,js.map,d.ts,d.ts.map}",
        ".tsbuildinfo",
        "lib"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "node --enable-source-maps --test --test-reporter=spec \"test/**/*_test.js\"",
      "dependencies": [
        "build"
      ],
      "files": []
    }
  },
  "dependencies": {
    "@trpc/server": "^10.45.2",
    "@zipadee/core": "^0.0.1"
  }
}
